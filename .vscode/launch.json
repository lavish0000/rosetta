{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Rosetta",
            "cwd": "${workspaceRoot}",
            "program": "${workspaceFolder}/src/app/index.ts",
            "sourceMaps": true,
            "outFiles": [
                "${workspaceFolder}/dist/app/*.js"
            ],
            "env": {
                "NETWORK":"test",
                "THORNODE_VERSION":"v1.3.6",
                "MODE":"online"
            }
        }
    ]
}